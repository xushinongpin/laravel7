{
    "name": "codingyu/ueditor",
    "description": "UEditor extension for laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "ueditor"],
    "homepage": "https://github.com/codingyu/UEditor",
    "license": "MIT",
    "authors": [
        {
            "name": "codingyu",
            "email": "everceyu@gmail.com"
        }
    ],
    "require": {
